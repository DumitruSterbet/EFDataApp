@model EFDataApp.Models.Note
@{
    ViewBag.Title = "Modificarea nota student";
}

<form class="bg-white" asp-action="Change" asp-controller="curs" asp-route-id="@Model.StudentId">
    <div class="form-group">
        <label asp-for="Nota" class="control-label">Introdu nota</label>
        <input type="number" asp-for="Nota" class="form-control" />
    </div>
    <h6>@ViewBag.NoteErr</h6>
  
    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-default" />
    </div>
</form>
