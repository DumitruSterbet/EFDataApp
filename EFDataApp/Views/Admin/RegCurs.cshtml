@model EFDataApp.Models.Curs
@{
    ViewBag.Title = "Adaugarea cursului";
}
<form asp-action="RegCurs" asp-controller="Admin">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Denumirea</label>
        <input type="text" asp-for="Name" class="form-control" />
        <span asp-validation-for="Name"></span>
    </div>
    <div class="form-group">
        <label asp-for="About" class="control-label">Despre</label>
        <input type="text" asp-for="About" class="form-control" />
        <span asp-validation-for="About"></span>
    </div>
    <div class="form-group">
        <label asp-for="Profesor" class="control-label">Professor</label>
        <input type="text" asp-for="Profesor" class="form-control" />
        <span asp-validation-for="Profesor"></span>
    </div>
   
    <div class="form-group">
        <input type="submit" value="Trimite" class="btn btn-default" />
    </div>
</form>
